%li.generic.thumb{class: admin.processed == false && "working"}
	%a.thumb{href: admin_video_path(admin), title: admin.moderation_note}
		.tile
			%img(loading="lazy"){src: admin.tiny_thumb(current_user, current_filter)}
			%span.tag-set.duration= duration admin.duration
		.title
			- if admin.hidden
				%span.icon(title="Private - This item will not appear in public listings")= fa :lock
			= admin.title
		%span.date{title: admin.created_at}= since admin.created_at
		%span.tag-set.right
			- if admin.liked?
				= fa "thumbs-#{admin.disliked? ? "down red" : "up green"}"
			= number_with_delimiter admin.views
			views
