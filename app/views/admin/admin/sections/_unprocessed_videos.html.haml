%h1.title.resize-target
	= fa :film
	Unprocessed Videos
	= surround '(', ')' do
		= number_with_delimiter @unprocessed.count
	%a.right{href: videos_path(unprocessed: 1)}
		View All
		= fa :chevron_right
- if @unprocessed.count > 0
	= render layout: 'pagination/paginated', locals: {type: admin_videos_unprocessed_index_path, paginated: @unprocessed, id: "unprocessed"} do
		%ul.horizontal.video-list#videos
			= render partial: 'videos/thumb/admin', collection: @unprocessed.records
